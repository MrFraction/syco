# Password policy
dn: cn=config
changetype: modify
add: passwordChange
passwordChange: on

add: passwordExp
passwordExp: on

add: passwordMaxAge
passwordMaxAge: 8640000

add: passwordCheckSyntax
passwordCheckSyntax: on

add: passwordMinCategories
passwordMinCategories: 3

add: passwordStorageScheme
passwordStorageScheme: SHA-512